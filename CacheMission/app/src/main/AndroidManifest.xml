<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"



    package="com.selectstar.hwshin.cashmission">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>



    <application
        android:allowBackup="true"
        android:icon="@drawable/cashmissionicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cashmissioniconround"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.selectstar.hwshin.cashmission.Activity.TaskListActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"

            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.TaskActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustResize"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.SignUpActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.SettingActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.ExamActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.SuggestionActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.TaskExplainActivity"
            android:theme="@style/AppTheme.Transparent.NoActionBar"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.ExchangeActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.ExplainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.RegionActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.TestActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            android:hardwareAccelerated="false"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.NoticeActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>
        <activity android:name="com.selectstar.hwshin.cashmission.Activity.NoticePopUpActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"
            ></activity>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="com.selectstar.hwshin.cashmission"
            android:exported="false"
            android:grantUriPermissions="true">

            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>