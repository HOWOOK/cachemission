<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-700dp"
        android:right="-700dp">
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90">
            <shape
                android:shape="line">
                <stroke
                    android:width="2.5dp"
                    android:color="#000000"
                    android:dashWidth="6dp"
                    android:dashGap="5dp" />
            </shape>
        </rotate>
    </item>
</layer-list>